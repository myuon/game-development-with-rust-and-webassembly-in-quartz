// ----
// js library
fun js_console_log(message: string) {
    _console_log(JsString::new(message).data);
}

fun js_window(): JsRef {
    return JsRef {
        data: _window(),
    };
}

fun js_window_document(window: JsRef): JsRef {
    return JsRef {
        data: _window_document(window.data),
    };
}

