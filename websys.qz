struct JsRef {
    data: i32,
}

struct JsString {
    data: ptr[byte], // ciovec
}

module JsString {
    fun new(s: string): JsString {
        let ciovec = make[ptr[byte]](8);
        set_ciovec(ciovec, s.data, s.length);

        return JsString {
            data: ciovec,
        };
    }
}

